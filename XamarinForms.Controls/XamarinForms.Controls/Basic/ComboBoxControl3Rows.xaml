<?xml version="1.0" encoding="utf-8"?>

<ContentView x:Class="XamarinForms.Controls.Basic.ComboBoxControl3Rows"
			 xmlns="http://xamarin.com/schemas/2014/forms"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
